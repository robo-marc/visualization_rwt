<launch>
  <arg name="launch_roswww" default="true" />
  <arg name="display" default=":102" />
  <include file="$(find rwt_graph)/launch/www.launch" if="$(arg launch_roswww)" />

  <node pkg="rwt_graph" type="xvnc.sh" args="$(arg display)" name="xvnc" output="screen" />
  <node pkg="rwt_graph" type="launch_websockify.sh" args="$(arg display)" name="websocktify" output="screen" />
  <node pkg="rwt_graph" type="wm.sh" args="-f $(find rwt_graph)/config/ratpoisonrc" name="wm" output="screen">
    <env name="DISPLAY" value="$(arg display)" />
  </node>
  <node pkg="rqt_graph" type="rqt_graph" name="rviz" output="screen" respawn="true" respawn_delay="1">
    <env name="DISPLAY" value="$(arg display)" />
  </node>
</launch>
